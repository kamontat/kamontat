  "version": "0.3.0-beta.1",
  "license": "BSD-3-Clause",
  "keywords": [
    "gatsby",
    "portfolio",
    "personal",
    "typescript",
    "website"
  ],
  "author": {
    "name": "Kamontat Chantrachirathumrong",
    "email": "developer@kamontat.net",
    "url": "https://kamontat.net"
  },
  "repository": {
    "type": "git",
