  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ignore-path .gitignore .",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cy:open": "cypress open --config-file cypress/config.json",
    "cy:run": "cypress run --config-file cypress/config.json",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "coverage": "jest --coverage",
    "coverage:ci": "jest --coverage --ci --runInBand",
    "test:e2e": "start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test dev http://localhost:8000 cy:run"
  },
